

<%= form_for([@course, @objective]) do |f| %>
	
	<%= f.label :name %>
    <%= f.text_field :name %> 
    <%= link_to 'Add New',  course_objectives_path(@course), class: 'button',
    	method: :post %>
	
<% end %>



<h1>Objective</h1>

<% @objectives.each do |objective|%>
		<li> <%= objective.name %></li>
		<%= link_to "Edit", edit_course_objective_path(@course, objective)%>
		<%= link_to "Delete", course_objective_path(@course, objective),
					method: :delete, data: {confirm: 'Are you sure'} %>		
<% end %>